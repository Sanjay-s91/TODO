<h1 class ="user">Search task</h1>
 <div class="container">
  <table class = "table table-striped table-dark table-bordered table-hover">
    <tbody>
      <% @results.each do |result| %>
         <tr>
         	<%if result.user_id == session[:user_id]%>
              <td><%= result.task_note %></td>
            <%end%>
         </tr>
      <% end %>
    </tbody>
  </table>
</div>